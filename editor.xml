<application>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="bbaa46" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bhelp\b.*">
      <option name="FOREGROUND" value="46bb6e" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
  </component>
</application>